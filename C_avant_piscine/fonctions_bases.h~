// Directives de preprocesseur
#ifndef DEF_FONCTIONS_BASES
	#define DEF_FONCTIONS_BASES
	#include <stdio.h>
	#include <stdlib.h>
#endif

void afficherTableau(int *tableau, int tailleTableau)
{
	printf("\nLes valeurs du tableau sont {");
	for(int i = 0;i<tailleTableau;i++)
	{
		printf("%d, ", tableau[i]);
	}
	printf("}\n");
}

